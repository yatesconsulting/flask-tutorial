set nocompatible
" source $VIMRUNTIME/vimrc_example.vim
" C:\Program Files (x86)\Vim\vim81\vimrc_example.vim
behave xterm
set selectmode=mouse
set guioptions+=a
set shiftwidth=2
set ignorecase smartcase
